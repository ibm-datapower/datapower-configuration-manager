<?xml version="1.0"?>
<project name="troubleshooting-send-log-event" basedir="." default="troubleshooting-send-log-event">
    
    <!--
        Generate a log event.
    -->
    <import file="common-definitions.ant.xml" optional="no"/>
	
    <target name="troubleshooting-send-log-event">
        
    
        <wdp operation="SendLogEvent" dumpinput="${dumpinput}" dumpoutput="${dumpoutput}" capturesoma="${capturesoma}">
            <logcategory>xsltmsg</logcategory>
            <loglevel>error</loglevel>
            <msg>Fake message generated by troubleshooting-send-log-event.ant.xml</msg>
            <hostname>${hostname}</hostname>
            <uid>${uid}</uid>
            <pwd>${pwd}</pwd>
            <domain>${domain}</domain>
        </wdp>
        
        <echo>Successfully generated a log event in ${domain} on ${hostname}</echo>

    </target>
    
</project>
